{
  "name": "support-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "pre-commit": "validate-branch-name && lint-staged -q",
    "dev": "next dev",
    "build": "next build && cp -r .next/static .next/standalone/.next/ && cp -r public .next/standalone/",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch --onlyChanged",
    "test:report": "open-cli html-report/report.html",
    "test:cover": "jest --coverage",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/poppins": "^5.1.1",
    "@hookform/resolvers": "^3.9.0",
    "@mui/icons-material": "^5.15.12",
    "@mui/lab": "^5.0.0-alpha.167",
    "@mui/material": "^5.18.0",
    "@mui/x-data-grid": "^6.19.6",
    "@mui/x-date-pickers": "^6.19.6",
    "@sanity/image-url": "^1.0.2",
    "@sanity/portable-text-editor": "^3.45.0",
    "@sanity/react-loader": "^1.10.2",
    "@tanstack/react-query": "^5.37.1",
    "axios": "^1.6.2",
    "msw": "^1.3.2",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "next-sanity": "^9.3.10",
    "react": "^18",
    "react-day-picker": "^9.7.0",
    "react-dom": "^18",
    "react-hook-form": "^7.51.5",
    "react-icons": "^5.5.0",
    "react-table": "^7.8.0",
    "sass": "^1.86.3",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": ">=18 <21",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-expect-message": "^1.1.3",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "jest-mock": "^29.7.0",
    "jest-svg-transformer": "^1.0.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^15.2.2",
    "open-cli": "^8.0.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "validate-branch-name": "^1.3.0",
    "whatwg-fetch": "^3.6.20"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "prettier": {
    "semi": true,
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "validate-branch-name": {
    "pattern": "(feat|fix|docs|build)\\/(\\d+-)[a-z-]+",
    "errorMsg": "Avoid committing without a branch"
  }
}

